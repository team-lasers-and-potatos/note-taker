<h1>Notes</h1>
<div class="row">
  <% @notes.each do |f| %>
    <div class="col-md-4">
      <div class="bord">
        <h2><%= link_to f.title, note_path(f.id) %></h2>
        <p><%= link_to 'Show', note_path(f.id) %></p>
        <p><%= f.body %></p>
        <ul>
          <li><%= link_to "edit" %></li>

        </ul>
      </div>
    </div>
  <% end %>
</div>
