
  <div class="col-md-4 col-md-offset-4">
    <div class="row">
      <ul class="notes">
        <li>
          <div class="rotate-2 yellow-bg">
            <%= form_for(@notes) do |f| %>

              <%= f.label :title %>
              <%= f.text_field :title %>

              <%= f.label :body %>
              <%= f.text_area :body %>
              <br>
              <%= f.hidden_field :user_id %>

              <%= f.submit "Create Note", class: 'btn btn-primary'  %>


            </div>
          </li>
        </ul>
    </div>
  </div>
<% end %>
